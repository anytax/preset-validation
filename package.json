{
  "name": "@anytax/preset-validation",
  "version": "1.0.0",
  "description": "A comprehensive validation library for German tax identifiers and international banking codes",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "test": "jest",
