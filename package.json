  "author": "AnyTax",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/anytax/preset-validation.git"
  },
  "bugs": {
    "url": "https://github.com/anytax/preset-validation/issues"
  },
  "homepage": "https://github.com/anytax/preset-validation#readme",
  "files": [
    "dist",
    "src",
    "tsconfig.json",
    "README.md",
    "LICENSE"
  ],
  "dependencies": {
    "typescript": "^5.0.0"
  },
  "devDependencies": {
    "@types/jest": "^29.0.0",
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.0.0",
    "jest": "^29.0.0",
